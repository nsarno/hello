{
  "containerDefinitions": [
    {
      "name": "hello",
      "image": "nsarno/hello:v1",
      "memory": 128,
      "portMappings": [{
        "containerPort": 4000,
        "hostPort": 80,
        "protocol": "tcp"
      }],
      "essential": true,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "awslogs-hello",
          "awslogs-region": "us-east-1"
        }
      }
    }
  ],
  "family": "hello"
}
